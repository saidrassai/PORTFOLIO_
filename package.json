{
  "name": "changelog_portfolio",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "build:prod": "tsc -b && vite build --mode production",
    "analyze": "npm run build && node scripts/bundle-analysis.js",
    "bundle-analysis": "node scripts/bundle-analysis.js",
    "serve": "vite preview --port 4173",
    "convert-icons": "node scripts/convert-icons.js",
    "optimize-images": "npm run convert-icons",
    "optimize-models": "node scripts/optimize-models.js",
    "extract-critical-css": "node scripts/extract-critical-css.js",
    "optimize-critical-css": "node scripts/optimize-critical-css.js",
    "performance-summary": "node scripts/performance-summary.js",
    "optimize-css": "npm run optimize-critical-css",
    "perf-test": "lighthouse http://localhost:4173 --output html --output-path ./lighthouse-report.html",
    "perf-ci": "lhci autorun",
    "test:perf": "npm run build && npm run serve & sleep 5 && npm run perf-test && kill %1"
  },
  "dependencies": {
    "@react-three/drei": "^10.1.2",
    "@react-three/fiber": "^9.1.2",
    "@react-three/postprocessing": "^3.0.4",
    "@react-three/rapier": "^2.1.0",
    "@types/react-google-recaptcha": "^2.1.9",
    "gsap": "^3.13.0",
    "lucide-react": "^0.511.0",
    "maath": "^0.10.8",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-google-recaptcha": "^3.1.0",
    "tailwindcss": "^3.4.17",
    "three": "^0.177.0",
    "web-vitals": "^5.0.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@lhci/cli": "^0.14.0",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "gltf-pipeline": "^4.1.0",
    "lighthouse": "^12.6.1",
    "postcss": "^8.5.4",
    "puppeteer": "^24.10.0",
    "sharp": "^0.34.2",
    "terser": "^5.41.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5",
    "vite-bundle-analyzer": "^0.22.0",
    "webpack-bundle-analyzer": "^4.10.2"
  }
}
